<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <security>
            <requestFiltering>
                <fileExtensions>
                    <add fileExtension=".css" allowed="true" />
                    <add fileExtension=".js" allowed="true" />
                    <add fileExtension=".less" allowed="true" />
                    <add fileExtension=".ico" allowed="true" />
                </fileExtensions>
            </requestFiltering>
        </security>
        <staticContent>
            <mimeMap fileExtension=".less" mimeType="text/css" />
        </staticContent>
    </system.webServer>
</configuration>
